    }


    var time_region = 0;
    //点击地区的select，获取地区列表
    $(document).on("click", ".regionclick", function () {
        var str = "";
        var that = this;
        if (time_region == 0) {
            $.ajax("/api/region_main", {
                method: "get",
            }).done(function (res) {
                if (res && res.data !== undefined) {
